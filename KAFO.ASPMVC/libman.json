{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-rtl@3.4.0",
      "destination": "wwwroot/lib/bootstrap-rtl/"
    },
    {
      "library": "font-awesome@6.7.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "jsdelivr",
      "library": "@microsoft/signalr@6.0.6",
      "destination": "wwwroot/microsoft/signalr/",
