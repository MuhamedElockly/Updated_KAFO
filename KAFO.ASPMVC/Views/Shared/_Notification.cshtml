<link href="~/CCDN/toastr.min.css" rel="stylesheet" />


<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/CCDN/toastr.min.js"></script>

@if (TempData["success"] != null)
{
    
    <script type="text/javascript">
        toastr.success('@TempData["success"]');

    </script>

}else if (TempData["erorr"] != null)
{
    <script type="text/javascript">
        toastr.success('@TempData["erorr"]');

    </script>
}